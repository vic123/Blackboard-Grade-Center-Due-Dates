<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
          "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">

<struts-config>
    
  <form-beans>
<!-- Gradebook2 -->

    <form-bean name="AddModifyItemDefinitionForm"
               type="blackboard.webapps.gradebook2.struts.forms.AddModifyItemDefinitionForm"/>
    <form-bean name="AddModifyGradingSchemaForm"
               type="blackboard.webapps.gradebook2.struts.forms.AddModifyGradingSchemaForm"/>
    <form-bean name="ModifyDefaultGradingSchemaForm"
               type="blackboard.webapps.gradebook2.struts.forms.ModifyDefaultGradingSchemaForm"/>
    <form-bean name="ViewGradeDetailsForm"
               type="blackboard.webapps.gradebook2.struts.forms.ViewGradeDetailsForm"/>
    <form-bean name="ModifyGradeForm"
               type="blackboard.webapps.gradebook2.struts.forms.ModifyGradeForm"/>
    <form-bean name="composeEmailForm"
               type="blackboard.webapps.gradebook2.struts.forms.ComposeEmailForm"/>
    <form-bean name="AddModifyGradingPeriodsForm"
               type="blackboard.webapps.gradebook2.struts.forms.AddModifyGradingPeriodsForm"/>
    <form-bean name="AddModifyCategoryForm"
               type="blackboard.webapps.gradebook2.struts.forms.AddModifyCategoryForm"/>
	<form-bean name="downloadGradebookForm"
               type="blackboard.webapps.gradebook2.struts.forms.DownloadGradebookForm"/>
	<form-bean name="uploadGradebookForm2"
               type="blackboard.webapps.gradebook2.struts.forms.UploadGradebookForm"/>
	<form-bean name="gradingPeriodLayoutForm"
               type="blackboard.webapps.gradebook2.struts.forms.GradingPeriodLayoutForm"/>
	<form-bean name="viewSpreadsheet2Form"
               type="blackboard.webapps.gradebook2.struts.forms.ViewSpreadsheet2Form"/>

    <form-bean name="manageCumulativeItemForm"
               type="blackboard.webapps.gradebook2.struts.forms.ManageCumulativeItemForm"/>    
    <form-bean name="AddModifyCustomViewsForm"
               type="blackboard.webapps.gradebook2.struts.forms.AddModifyCustomViewsForm"/>

    <form-bean name="GenerateReportForm"
               type="blackboard.webapps.gradebook2.struts.forms.GenerateReportForm"/>
    <form-bean name="DisplayReportForm"
               type="blackboard.webapps.gradebook2.struts.forms.DisplayReportForm"/>
    <form-bean name="ModifyCommentsForm"
               type="blackboard.webapps.gradebook.struts.forms.ModifyCommentsForm"/>
    <form-bean name="downloadAssignmentForm" type="blackboard.webapps.gradebook2.struts.forms.DownloadAssignmentForm"/>
    <form-bean name="purgeAssignmentForm" type="blackboard.webapps.gradebook2.struts.forms.PurgeAssignmentForm"/>

  </form-beans>
  
  <global-forwards>
    <forward name="error"
             path="/WEB-INF/jsp/error.jsp"/>
    <forward name="/student/viewGrades"
             path="/do/student/viewGrades"/>
    <forward name="/student/viewCourses"
             path="/do/student/viewCourses"/>
  </global-forwards>
  
  <action-mappings>
    <action path="/student/viewGrades" 
            type="blackboard.webapps.gradebook.struts.actions.ViewStudentGradesAction">
      <forward name="next" path="/WEB-INF/jsp/gradebook/student/view-grades.jsp"/>
    </action>
    <action path="/student/viewAttempts" 
            type="blackboard.webapps.gradebook.struts.actions.ViewStudentAttemptsAction">
      <forward name="list" path="/WEB-INF/jsp/gradebook/student/view-attempts.jsp"/>
      <forward name="comment" path="/WEB-INF/jsp/gradebook/student/view-comment.jsp"/>
    </action>
    
    <action path="/student/viewCourses"
            type="blackboard.webapps.gradebook.struts.actions.ViewStudentCoursesAction">
      <forward name="next" path="/WEB-INF/jsp/gradebook/student/view-courses.jsp"/>
    </action>
    
<!--  Gradebook 2 -->
    <action path="/instructor/enterGradeCenter" 
            type="blackboard.webapps.gradebook2.struts.actions.EnterGradeCenterAction">
      <forward name="noFrameGradeCenter" path="/do/instructor/viewSpreadsheet2"/>
      <forward name="loadModelAndGoTo" path="/gradebook2/instructor/load_model_and_go_to.jsp"/>
      <!-- in Grade Center SSL Mode we need a frame to have a more global scope for the grade center application.
           This scope cannot be the course frameset since it would be http and cannot be accessed from
           an https page (Javascript Same Domain security policy) -->
      <forward name="inframeGradeCenter" path="/gradebook2/instructor/gradecenter_framecheck.jsp"/>
    </action>
    <action path="/instructor/viewSpreadsheet2" 
            name="viewSpreadsheet2Form"
            type="blackboard.webapps.gradebook2.struts.actions.ViewSpreadsheet2Action">
      <forward name="next" path="/gradebook2/instructor/view-spreadsheet2.jsp"/>
    </action>
    <action path="/instructor/getJSONData" 
            type="blackboard.webapps.gradebook2.struts.actions.GetJSONDataAction">
      <forward name="next" path="/WEB-INF/jsp/gradebook2/instructor/get-json_data.jsp"/>
    </action>
    <action path="/instructor/addModifyItemDefinition"
            type="blackboard.webapps.gradebook2.struts.actions.AddModifyItemDefinitionAction"
            name="AddModifyItemDefinitionForm"
            input="/gradebook2/instructor/addModifyItemDefinition.jsp"
            validate="true"
            parameter="method">
      <forward name="create" path="/gradebook2/instructor/addModifyItemDefinition.jsp"/>
      <forward name="modify" path="/gradebook2/instructor/addModifyItemDefinition.jsp"/>
      <forward name="save" path="/gradebook2/instructor/addModifyItemDefinition.jsp"/>
      <forward name="cancel" path="/do/instructor/viewSpreadsheet2"/>
    </action>	
    <action path="/instructor/addModifySchema"
            type="blackboard.webapps.gradebook2.struts.actions.AddModifyGradingSchemaAction"
            name="AddModifyGradingSchemaForm"
            input="/gradebook2/instructor/add_modify_grading_schema.jsp"
            validate="true"
            parameter="method">
      <forward name="form" path="/gradebook2/instructor/add_modify_grading_schema.jsp"/>
      <forward name="next" path="/do/instructor/manageGradingSchema"/>
    </action>	

    <action path="/instructor/modifyDefaultSchema"
            type="blackboard.webapps.gradebook2.struts.actions.ModifyDefaultGradingSchemaAction"
            name="ModifyDefaultGradingSchemaForm"
            input="/gradebook2/instructor/modify_default_grading_schema.jsp"
            validate="true"
            parameter="method">
      <forward name="form" path="/gradebook2/instructor/modify_default_grading_schema.jsp"/>
    </action>	

    <action path="/instructor/manageGradingSchema" 
            type="blackboard.webapps.gradebook2.struts.actions.ManageGradingSchemaAction"
            input="/gradebook2/instructor/manage_grading_schema.jsp"
            validate="true">
      <forward name="next" path="/gradebook2/instructor/manage_grading_schema.jsp"/>
    </action>
    <action path="/instructor/deleteItem" 
            type="blackboard.webapps.gradebook2.struts.actions.DeleteItemAction">
      <forward name="next" path="/do/instructor/viewSpreadsheet2"/>
    </action>
    <action path="/instructor/viewGradeDetails"
            type="blackboard.webapps.gradebook2.struts.actions.ViewGradeDetailsAction"
            scope="request"
            validate="false"
            name="ViewGradeDetailsForm">
      <forward name="next" path="/gradebook2/instructor/view_grade_details.jsp"/>
      <forward name="error" path="/do/instructor/viewSpreadsheet2"/>
    </action>
    <action path="/instructor/modifyGrade2" 
            type="blackboard.webapps.gradebook2.struts.actions.ModifyGradeAction"
            scope="request"
            input="/gradebook2/instructor/view_grade_details.jsp"
            validate="true"
            name="ModifyGradeForm">
      <forward name="next" path="/do/instructor/viewGradeDetails"/>
    </action>
    <action path="/instructor/clearAttempt" 
            type="blackboard.webapps.gradebook2.struts.actions.ClearAttemptAction">
      <forward name="next" path="/do/instructor/viewGradeDetails"/>
      <forward name="spreadsheet" path="/do/instructor/viewSpreadsheet2"/>
    </action>
    <action path="/instructor/addExtraAttempt" 
            type="blackboard.webapps.gradebook2.struts.actions.AddExtraAttemptAction">
      <forward name="next" path="/do/instructor/viewGradeDetails"/>
    </action>
    <action path="/instructor/getGradeHistory" 
            type="blackboard.webapps.gradebook2.struts.actions.GradeHistoryAction">
      <forward name="next" path="/gradebook2/instructor/grade_history.jsp"/>
      <forward name="spreadsheet" path="/do/instructor/viewSpreadsheet2"/>
    </action>
     
    <action path="/instructor/composeEmail"
            type="blackboard.webapps.gradebook2.struts.actions.ComposeEmailAction"
            name="composeEmailForm"
            scope="request"
            validate="false">
      <forward name="composeEmail" path="/gradebook2/instructor/email_compose.jsp" />
    </action>
    
    <action path="/instructor/sendEmail"
            type="blackboard.webapps.gradebook2.struts.actions.SendEmailAction"
            name="composeEmailForm"
            input="/instructor/email_compose.jsp"
            scope="request"
            validate="false">
      <forward name="sendEmail" path="/gradebook2/instructor/view-spreadsheet2.jsp" />
    </action>
    
    <action path="/instructor/downloadGradebook"
            type="blackboard.webapps.gradebook2.struts.actions.DownloadGradebookAction"
            scope="request"
            parameter="dispatch"
            name="downloadGradebookForm"
            input="/gradebook2/instructor/download.jsp"
            validate="true">
      <forward name="downloadChoice" path="/gradebook2/instructor/download_choice.jsp" />
      <forward name="download" path="/gradebook2/instructor/download.jsp" />
      <forward name="histryDownloadChoice" path="/gradebook2/instructor/history_download_choice.jsp" />
      <forward name="historyDownload" path="/gradebook2/instructor/history_download.jsp" />
      <forward name="viewSpreadSheet" path="/do/instructor/viewSpreadsheet2"/>
    </action>

    <action path="/instructor/uploadGradebook2"
            type="blackboard.webapps.gradebook2.struts.actions.UploadGradebookAction"
            scope="request"
            name="uploadGradebookForm2"
            validate="true">
      <forward name="upload" path="/gradebook2/instructor/uploadFileSelection.jsp" />
      <forward name="viewSpreadSheet" path="/do/instructor/viewSpreadsheet2"/>
      <forward name="confirm" path="/gradebook2/instructor/upload_gradbook_confirmation.jsp" />
    </action>
  
  
  
      <action path="/instructor/addModifyPeriods"
            type="blackboard.webapps.gradebook2.struts.actions.AddModifyGradingPeriodsAction"
            name="AddModifyGradingPeriodsForm"
            input="/gradebook2/instructor/add_modify_grading_periods.jsp"
            validate="true"
            parameter="method">
      <forward name="form" path="/gradebook2/instructor/add_modify_grading_periods.jsp"/>
      <forward name="next" path="/do/instructor/manageGradingPeriods"/>
    </action>	
    
    <action path="/instructor/manageGradingPeriods" 
            type="blackboard.webapps.gradebook2.struts.actions.ManageGradingPeriodsAction"
            validate="true">
      <forward name="next" path="/gradebook2/instructor/manage_grading_periods.jsp"/>
    </action>
    
    <action path="/instructor/manageCategories" 
            type="blackboard.webapps.gradebook2.struts.actions.ManageCategoriesAction"
            input="/gradebook2/instructor/manage_categories.jsp">
      <forward name="next" path="/gradebook2/instructor/manage_categories.jsp"/>
    </action>
    
    <action path="/instructor/addModifyCategory"
            type="blackboard.webapps.gradebook2.struts.actions.AddModifyCategoryAction"
            name="AddModifyCategoryForm" scope="request"
            input="/gradebook2/instructor/add_modify_category.jsp"
            validate="true"
            parameter="method">
      <forward name="form" path="/gradebook2/instructor/add_modify_category.jsp"/>
      <forward name="next" path="/do/instructor/manageCategories"/>
    </action>	
    
    <action path="/instructor/manageStudentVisibility" 
            type="blackboard.webapps.gradebook2.struts.actions.ManageStudentVisibilityAction">
      <forward name="show" path="/gradebook2/instructor/manage_students.jsp"/>
      <forward name="viewSpreadSheet" path="/do/instructor/viewSpreadsheet2"/>
    </action>
    
    <action path="/instructor/gradingPeriodLayout" 
            name="gradingPeriodLayoutForm"
            type="blackboard.webapps.gradebook2.struts.actions.GradingPeriodLayoutAction">
      <forward name="form" path="/gradebook2/instructor/grading_period_layout.jsp"/>
      <forward name="next" path="/do/instructor/viewSpreadsheet2"/>
    </action>
    
    <action path="/instructor/manageCumulativeItem" 
            type="blackboard.webapps.gradebook2.struts.actions.ManageCumulativeItemAction"
            scope="request"
            parameter="dispatch"
            input="/gradebook2/instructor/manage_cumulative_item.jsp"
            validate="true"
     		name="manageCumulativeItemForm">
      <forward name="show" path="/gradebook2/instructor/manage_cumulative_item.jsp"/>
      <forward name="viewSpreadSheet" path="/do/instructor/viewSpreadsheet2"/>
    </action>
  
  
    <action path="/instructor/addModifyCustomViews"
            type="blackboard.webapps.gradebook2.struts.actions.AddModifyCustomViewsAction"
            name="AddModifyCustomViewsForm"
            input="/gradebook2/instructor/add_modify_custom_view.jsp"
            validate="true"
            parameter="method">
      <forward name="form" path="/gradebook2/instructor/add_modify_custom_view.jsp"/>
      <forward name="next" path="/do/instructor/manageCustomViews"/>
    </action>	
    
    <action path="/instructor/manageCustomViews" 
            type="blackboard.webapps.gradebook2.struts.actions.ManageCustomViewsAction">
      <forward name="next" path="/gradebook2/instructor/manage_custom_views.jsp"/>
    </action>  

    <action path="/instructor/generateReport"
            type="blackboard.webapps.gradebook2.struts.actions.GenerateReportAction"
            name="GenerateReportForm"
            parameter="method">
      <forward name="create_page" path="/gradebook2/instructor/generate_report.jsp"/>
      <forward name="next" path="/do/instructor/viewSpreadsheet2"/>
    </action>	

    <action path="/instructor/viewReport"
            type="blackboard.webapps.gradebook2.struts.actions.DisplayGradebookReport"
            name="DisplayReportForm"
            parameter="method">
      <forward name="display" path="/gradebook2/instructor/display_gb_report.jsp"/>
    </action>	

    <action path="/instructor/viewItemStats" 
            type="blackboard.webapps.gradebook2.struts.actions.ViewItemStatsAction">
      <forward name="next" path="/gradebook2/instructor/view_item_stats.jsp"/>
    </action>  

    <action path="/instructor/viewStudentStats" 
            type="blackboard.webapps.gradebook2.struts.actions.ViewStudentStatsAction">
      <forward name="next" path="/gradebook2/instructor/view_student_stats.jsp"/>
    </action>  

    <action path="/instructor/modifyComments" 
            type="blackboard.webapps.gradebook.struts.actions.ModifyCommentsAction"
            name="ModifyCommentsForm"
            input="/WEB-INF/jsp/gradebook/instructor/modifyComments.jsp"
            validate="true"
            parameter="method">
      <forward name="closeWindow" path="/WEB-INF/jsp/gradebook/instructor/closeWindow.jsp"/>
    </action>
    
            <!-- Download Assignment/GroupAssignment -->
    <action path="/instructor/downloadAssignment"
            type="blackboard.webapps.gradebook2.struts.actions.DownloadAssignmentAction"
            name="downloadAssignmentForm"
            scope="request"
            input="/gradebook2/instructor/download_assignment.jsp"
            parameter="cmd"
            validate="false">
      <forward name="download" path="/gradebook2/instructor/download_assignment.jsp"/>
      <forward name="receipt" path="/gradebook2/instructor/download_assignment_receipt.jsp" redirect="false"/>
    </action>
    
        <!-- Purge Assignment/GroupAssignment -->
    <action path="/instructor/purgeAssignment"
            type="blackboard.webapps.gradebook2.struts.actions.PurgeAssignmentAction"
            name="purgeAssignmentForm"
            scope="request"
            input="/assignments/purge_assignment.jsp"
            parameter="cmd"
            validate="false">
      <forward name="purge" path="/gradebook2/instructor/purge_assignment.jsp"/>
    </action>

  </action-mappings>

  <message-resources parameter="blackboard.webapps.gradebook.struts.ApplicationResources"/>

</struts-config>
